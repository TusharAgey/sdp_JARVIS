<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jarvis.jarvis">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main"></activity>
        <activity android:name=".OneTimeHelp" />
        <activity android:name=".SignUp" />
        <activity android:name=".LoginPage" />
        <activity android:name=".HomePage" />
        <activity
            android:name=".HelpOfTime"
            android:label="@string/title_activity_help_of_time"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".StudyManager" />
        <activity android:name=".PasswordManager" />
        <activity android:name=".ContactManager" />
        <activity
            android:name=".MegaStart"
            android:label="@string/title_activity_mega_start"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayData"
            android:label="@string/title_activity_display_data"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Hologram"></activity>
    </application>

</manifest>